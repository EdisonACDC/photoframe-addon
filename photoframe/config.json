{
  "name": "PhotoFrame - Cornice Digitale BETA",
  "version": "1.0.28",
  "slug": "photoframe-beta",
  "description": "Trasforma il tuo tablet in una elegante cornice digitale con slideshow automatico",
  "arch": ["armhf", "armv7", "aarch64", "amd64", "i386"],
  "startup": "application",
  "boot": "auto",
  "ports": {
    "5000/tcp": 5000,
    "5443/tcp": 5443
  },
  "ports_description": {
    "5000/tcp": "HTTP Web Interface",
    "5443/tcp": "HTTPS Web Interface"
  },
  "ingress": true,
  "ingress_port": 5000,
  "ingress_entry": "/",
  "panel_icon": "mdi:image-multiple",
  "hassio_api": true,
  "homeassistant_api": false,
  "host_network": false,
  "options": {
    "ssl": false,
    "certfile": "fullchain.pem",
    "keyfile": "privkey.pem"
  },
  "schema": {
    "ssl": "bool",
    "certfile": "str",
    "keyfile": "str"
  }
}