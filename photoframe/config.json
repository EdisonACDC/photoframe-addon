{
  "name": "PhotoFrame - Cornice Digitale",
  "version": "1.0.16",
  "slug": "photoframe-v2",
  "description": "Trasforma il tuo tablet in una cornice digitale elegante con slideshow automatico",
  "url": "https://github.com/EdisonACDC/photoframe-addon",
  "arch": ["aarch64", "amd64", "armhf", "armv7", "i386"],
  "startup": "application",
  "boot": "auto",
  "init": false,
  "ports": {
    "5000/tcp": 5000
  },
  "ports_description": {
    "5000/tcp": "Interfaccia web PhotoFrame"
  },
  "webui": "http://[HOST]:[PORT:5000]",
  "ingress": true,
  "ingress_port": 5000,
  "panel_icon": "mdi:image-multiple",
  "panel_title": "PhotoFrame",
  "map": [
    "config:rw",
    "share:rw"
  ],
  "options": {
    "upload_limit": 50,
    "max_file_size": 10
  },
  "schema": {
    "upload_limit": "int(1,100)?",
    "max_file_size": "int(1,50)?"
  }
}
